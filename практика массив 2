using System;

namespace ConsoleApp2
{
class Program
{
static void Main(string[] args)
{
int n = Convert.ToInt32(Console.ReadLine());
int ma = -10000;
int mi = 100000;
var a = Array.ConvertAll(Console.ReadLine().Split(' '), n => int.Parse(n));
for (int i = 0; i < n; i++)
{
if (a[i] > ma)
{
ma = a[i];
}
if (a[i] < mi)
{
mi = a[i];
}
}
for (int i = 0; i < n; i++)
{
if (a[i] == ma)
{
a[i] = mi;
}
else
{
if (a[i] == mi)
{
a[i] = ma;
}
}
Console.WriteLine(a[i]);
}
}
}
}
